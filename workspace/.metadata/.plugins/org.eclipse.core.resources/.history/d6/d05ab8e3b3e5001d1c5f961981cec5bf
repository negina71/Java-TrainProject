package com.mci.model;

public enum Type {
List(Wagon.PASSENGER,Wagon.CARGO,Locomotive.ELECTRO,Locomotive.BENZIN,Locomotive.PETROL);
	
	private final Enum<?>[] enums;
	
	Type(Enum<?>...enums){
		this.enums=enums;
	}
	
	public Enum<?>[] getEnums(){
		return enums;
	}
	
	public enum Wagon{
		PASSENGER,CARGO
	}
	
	public enum Locomotive{
		ELECTRO,BENZIN,PETROL
	}
}
