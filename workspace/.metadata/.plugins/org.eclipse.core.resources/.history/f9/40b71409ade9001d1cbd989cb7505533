package com.mci.model;

import com.mci.model.Type.WagonType;

public class Wagon extends TrainComponents {

	private int counterOfPassengers;
	private double current_weight;
	private double LENGHT;
	private int MAX_CAPACITY;
	private double MIN_WEIGHT;
	private Type.WagonType type;

	
	
	public Wagon(String manufacturer, String serialNum, int year, int counterOfPassengers, double current_weight,
			double lENGHT, int mAX_CAPACITY, double mIN_WEIGHT, WagonType type) {
		super(manufacturer, serialNum, year);
		this.counterOfPassengers = counterOfPassengers;
		this.current_weight = current_weight;
		LENGHT = lENGHT;
		MAX_CAPACITY = mAX_CAPACITY;
		MIN_WEIGHT = mIN_WEIGHT;
		this.type = type;
	}
	
	public Wagon() {}

	@Override
	public double getCurrentWeight() {
		// TODO Auto-generated method stub
		return 0;
	}

	@Override
	public double getCurrentLenght() {
		// TODO Auto-generated method stub
		return 0;
	}

}
